    <fragment
        android:id="@+id/navigation_songs"
        android:name="com.movil.bellakkitys.ui.songs.SongsFragment"
        android:label="@string/title_songs"
        tools:layout="@layout/fragment_songs" />

    <fragment
        android:id="@+id/navigation_concerts"
